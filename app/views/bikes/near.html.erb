
<div class="top">
  <img class="bike_image" src="http://www.citibikenyc.com/assets/images/pages/feature-pages/about-the-bike/about-the-bike-module-1.png">
</div>



<%= @p %>

<table class="b_table">
    <tr>
        <th class="cell">Station</th>
        <th class="cell">Bikes</th>
        <th class="cell">Docks</th>
    </tr>
    <% @al.each do |station| %>
        <tr>
            <td class="cell"><%=station[0]["label"]%></td>
            <td class="cell"><%=station[0]["availableBikes"]%></td>
            <td class="cell"><%=station[0]["availableDocks"]%></td>
        </tr>
    <% end %>
</table>
